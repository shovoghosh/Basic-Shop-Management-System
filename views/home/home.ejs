<html>
<head>
	<title>Home | Page</title>
	
	<script  src="/js/jquery-3.3.1.js"></script>
	<script  src="/js/test.js"></script>
	<link rel="stylesheet" type="text/css" href="/css/style.css">
</head>
<body>
    <div class="home">
		<h1>Welcome <%=name %></h1>
		
		<a href="/home/create"> Add New Employee </a> 
		
		<a href="/product/list"> Product List </a> 
		<a href="/logout"> Log Out </a> 
		<br/><br/><br/>
		
		<input type="text" placeholder="Enter Employee Name" id="nam"/>
		<button>Search</button>
		<p id="par"></p>
		<br/><br/><br/>
		<table border="1">
			<tr>
				<th>Id</th>
				<th>User Name</th>
				<th>Contact No</th>
				<th>Operations</th>
			</tr>
			<% for(var i=0;i<list.length;i++){%>

				<tr>
					<td><%= list[i].id %></td>
					<td><%= list[i].username %></td>
					<td><%= list[i].contact %></td>
					<td> <a href="/home/edit/<%= list[i].id %>">Edit</a> 
					<a href="/home/delete/<%= list[i].id %>">Delete </a></td>
				</tr>



			<% } %>
		</table>
	</div>	
</body>
</html>